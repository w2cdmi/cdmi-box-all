        <property name="url" value="${jdbc.userdb_7.url}" />
    </bean>
	<bean id="userdb_8" class="pw.cdmi.common.alarm.AlarmAbleDataSource" destroy-method="close" parent="baseUserDB">
        <property name="url" value="${jdbc.userdb_8.url}" />
