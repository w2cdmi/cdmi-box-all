<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<Window dropenable="true">
	<VerticalLayout>
		<HorizontalLayout height="42" bkcolor="#FFF0F0F0">
			<HorizontalLayout>
				<Button name="teamSpace_back" padding="20,8,0,8" tooltip="后退" maxwidth="26" maxheight="26" normalimage="file='..\Image\ic_crumbs.png' source='0,108,26,134'" hotimage="file='..\Image\ic_crumbs.png' source='0,180,26,206'" disabledimage="file='..\Image\ic_crumbs.png' source='0,72,26,98'" enabled="false"/>
				<Button name="teamSpace_next" padding="5,8,0,8" tooltip="前进" maxwidth="26" maxheight="26" normalimage="file='..\Image\ic_crumbs.png' source='0,36,26,62'" hotimage="file='..\Image\ic_crumbs.png' source='0,144,26,170'" disabledimage="file='..\Image\ic_crumbs.png' source='0,0,26,26'" enabled="false"/>
				<GroupButton padding="10,6,0,6" name="teamSpace_groupBtn" textcolor="#FF008BE8" hottextcolor="#FF006AB0" disabledcolor="#FF999999"/>
			</HorizontalLayout>
			<HorizontalLayout  width="200" padding="0,6" height="30" name="teamSpace_fileSearchLayout" bkimage="file='..\Image\ic_input.png' source='0,0,40,30' corner='5,5,5,5'" visible="false">
				<SearchTxt name="teamSpace_fileSearchtxt" maxchar="246" align="left" padding="8,2" height="26" visible="true" wantreturn="true" autohscroll="true" font="10" valign="center" multiline="false" rich="false" textcolor="#FF999999" />				
				<Button name="teamSpace_clearsearchbtn" padding="5,7,10,0" width="16" height="16" normalimage="file='..\Image\ic_input_search_delete.png' source='0,0,16,16' dest='0,0,16,16'" hotimage="file='..\Image\ic_input_search_delete.png' source='0,26,16,42' dest='0,0,16,16'" pushedimage="file='..\Image\ic_input_search_delete.png' source='0,26,16,42' dest='0,0,16,16'" visible="false"/>
				<Button name="teamSpace_fileSearch" padding="0,5" showhtml="true" height="20" width="20" text="{i ..\Image\ic_input_search.png}{/i}" />
			</HorizontalLayout>
			<Control width="20"/>
		</HorizontalLayout>

		<HorizontalLayout height="1">
			<Label height="1" bkcolor="#FFE0E0E0" />
		</HorizontalLayout>

		<HorizontalLayout height="50">
			<VerticalLayout name="teamSpace_btngroup_level1" visible="false">
				<HorizontalLayout height="1" />
				<HorizontalLayout height="30"  padding="20,10">
					<ScaleIconButton name="teamSpace_create" borderround="3,3" text="创建团队空间"  font="0" float="false" textpadding="40,3,15,0" height="30" align="left" normalimage="file='..\Image\ic_button_color.png' source='0,0,80,30' corner='3,3,3,3'" hotimage="file='..\Image\ic_button_color.png' source='0,40,80,70' corner='3,3,3,3'" pushedimage="file='..\Image\ic_button_color.png' source='0,80,80,110' corner='3,3,3,3'"  iconimage="file='..\Image\ic_toolmenu_creatteam.png' dest='15,5,35,25'" iconhotimage="file='..\Image\ic_toolmenu_creatteam.png' dest='15,5,35,25'" textcolor="#FFFFFFFF"/>        
					<Button name="teamSpace_detail" borderround="3,3" enabled="false" visble="false" font="0" text="查看详情" float="false" padding="5,0,0,0" textpadding="40,3,0,0" height="30" width="108" align="left" normalimage="file='..\Image\ic_toolmenu_detail.png' source='0,0,20,20' dest='15,5,35,25'" hotimage="file='..\Image\ic_toolmenu_detail.png' source='0,30,20,50' dest='15,5,35,25'" pushedimage="file='..\Image\ic_toolmenu_detail.png' source='0,30,20,50' dest='15,5,35,25'"  textcolor="#FF008BE8" hottextcolor="#FF006AB0" disabledtextcolor="#FF999999"/>        
				</HorizontalLayout>
				<HorizontalLayout height="1" />
			</VerticalLayout>

			<VerticalLayout name="teamSpace_btngroup_level2" visible="true">		
				<HorizontalLayout height="1" />
				<HorizontalLayout height="50">
					<HorizontalLayout>
						<Text name="teamSpace_count" width="300" padding="20,17,0,17" font="2" align="left" visible="false"/>
						<Control width="1" height="30"/>
						<ScaleIconButton name="teamSpace_uploadFile"  padding="20,10,0,10" borderround="3,3" font="0"  text="上传" textpadding="40,4,15,0" align="left"  height="30"  normalimage="file='..\Image\ic_button_color.png' source='0,0,80,30' corner='3,3,3,3'" hotimage="file='..\Image\ic_button_color.png' source='0,40,80,70' corner='3,3,3,3'" pushedimage="file='..\Image\ic_button_color.png' source='0,80,80,110' corner='3,3,3,3'" iconimage="file='..\Image\ic_toolmenu_upload.png' source='0,0,20,20' dest='15,5,35,25'"  iconhotimage="file='..\Image\ic_toolmenu_upload.png' source='0,0,20,20' dest='15,5,35,25'" textcolor="#FFFFFFFF" />
						<ScaleIconButton name="teamSpace_createDir"   padding="10,10,0,10" borderround="3,3"  font="0" text="创建文件夹" textpadding="40,4,15,0" align="left"  height="30" normalimage="file='..\Image\ic_button_white_nor.png' source='0,0,80,30' corner='3,3,3,3'" hotimage="file='..\Image\ic_button_white_nor.png' source='0,40,80,70' corner='3,3,3,3'" pushedimage="file='..\Image\ic_button_white_nor.png' source='0,80,80,110' corner='3,3,3,3'" iconimage="file='..\Image\ic_toolmenu_creatfloder.png' source='0,0,20,20' dest='15,5,35,25'"  iconhotimage="file='..\Image\ic_toolmenu_creatfloder.png' source='0,0,20,20' dest='15,5,35,25'" textcolor="#FF008BE8"/>              
						<Button name="teamSpace_downFile"  enabled="false" visble="false" padding="20,10,0,10" borderround="3,3" font="0" text="下载" textpadding="25,5,0,0" align="left" width="52" height="30" normalimage="file='..\Image\ic_toolmenu_download.png' source='0,0,20,20' dest='0,5,20,25'" hotimage="file='..\Image\ic_toolmenu_download.png' source='0,30,20,50' dest='0,5,20,25'" disabledimage="file='..\Image\ic_toolmenu_download.png' source='0,0,20,20' dest='0,5,20,25'" textcolor="#FF008BE8" hottextcolor="#FF006AB0" disabledtextcolor="#FF999999"/>
						<Button name="teamSpace_more" menu="false" menuxmlfile="teamspaceContextMenu.xml" enabled="false" visble="false" padding="20,10,0,10" borderround="3,3" font="0" text="更多" textpadding="25,5,0,0" align="left"  width="52" height="30" normalimage="file='..\Image\ic_toolmenu_more.png' source='0,0,20,20' dest='0,5,20,25'" hotimage="file='..\Image\ic_toolmenu_more.png' source='0,30,20,50' dest='0,5,20,25'" disabledimage="file='..\Image\ic_toolmenu_more.png' source='0,0,20,20' dest='0,5,20,25'" textcolor="#FF008BE8" hottextcolor="#FF006AB0" disabledtextcolor="#FF999999"/>
					</HorizontalLayout>							
					<Control width="1"/>
				</HorizontalLayout>
				<HorizontalLayout height="1" />   
			</VerticalLayout>

			<HorizontalLayout name="teamSpace_switch" width="87" height="20" padding="0,15">
				<TabLayout>
					<HorizontalLayout >
						<Option name="teamSpace_filelist" tooltip="列表" width="20" group="teamSpace_fileChangeTable" normalimage="file='..\Image\ic_oper_list.png' source='0,0,20,20'" hotimage="file='..\Image\ic_oper_list.png' source='0,30,20,50'" pushedimage="file='..\Image\ic_oper_list.png' source='0,30,20,50'" selectedimage="file='..\Image\ic_oper_list.png' source='0,30,20,50'" selected="true"/>
						<Option name="teamSpace_largeIcon" tooltip="缩略图" width="20" padding="10,0" group="teamSpace_fileChangeTable" normalimage="file='..\Image\ic_oper_list_largeicon.png' source='0,0,20,20'" hotimage="file='..\Image\ic_oper_list_largeicon.png' source='0,30,20,50'" pushedimage="file='..\Image\ic_oper_list_largeicon.png' source='0,30,20,50'" selectedimage="file='..\Image\ic_oper_list_largeicon.png' source='0,30,20,50'" />
					</HorizontalLayout>
				</TabLayout>
				<Label name="teamSpace_devide"  padding="8,0" bkcolor="#FFD9D9D9" width="1" maxwidth="1" height="20" maxheight="20" />
				<Button name="teamSpace_update" padding="8,0" tooltip="刷新" shortcut="F5" width="20" normalimage="file='..\Image\ic_oper_refresh.png' source='0,0,20,20'" hotimage="file='..\Image\ic_oper_refresh.png' source='0,30,20,50'" disabledimage="file='..\Image\ic_oper_refresh.png' source='0,60,20,80'"/>
			</HorizontalLayout>
			<Control width="20"/>
		</HorizontalLayout>  
		<HorizontalLayout height="1"/>

		<HorizontalLayout name="teamSpace_Files" visible="true">
			<TabLayout name="teamSpace_listTab">
				<VerticalLayout>
					<!--团队显示-->			
					<TileLayoutList droptarget="false" name="teamSpace_tileLayout_listView" header="hidden" visible="true" mouse="true" childpadding="10" padding="5,0" encloseselect="true" itemsize="320,120"  itemshowhtml="true"  vscrollbar="true" hscrollbar="false"/>
				</VerticalLayout>
				<!--end 团队显示-->

				<VerticalLayout>
					<!--内容显示-->			
					<HorizontalLayout>
						<TabLayout name="teamSpace_listFileTab">
							<VerticalLayout>
								<CustomList name="teamSpace_FilelistView" menu="true" encloseselect="true" menuxmlfile="teamspaceContextMenu.xml" itemshowhtml="true" vscrollbar="true" hscrollbar="true" itemlinecolor="#FFE5E5E5" itemhotbkcolor="#FFEBEBEB" itemselectedbkcolor="#FFDEF0FF" inset="20,0,20,0">
									<ListHeader bkimage="file='..\Image\listheaderbk.png' corner='1,1,2,2'" height="28">
										<ListHeaderItem name="teamSpace_listheaderitem_name"  width="380" minwidth="380"  sepimage="..\Image\ic_tab_head_devideline.png" sepwidth="1">
											<HorizontalLayout mousechild="true" padding="10,0,2,0">
												<HorizontalLayout width="40">
													<Label name="teamSpace_listHeaderItemName" text="名称" font="2" textcolor="#FF666666"/>
													<Button name="teamSpace_listHeaderItemNameSortIcon" padding="5,11" width="6" height="6"  normalimage="file='..\Image\ic_tab_head_arrowup.png' source='0,0,6,6'" hotimage="file='..\Image\ic_tab_head_arrowup.png' source='0,16,6,22'"/>
												</HorizontalLayout>
											</HorizontalLayout>
										</ListHeaderItem>
										<ListHeaderItem name="teamSpace_listheaderitem_size"  width="120" minwidth="120" textcolor="#FF666666" sepimage="..\Image\ic_tab_head_devideline.png" sepwidth="1">
											<HorizontalLayout mousechild="true" padding="20,6,2,0">
												<Control />
												<HorizontalLayout>
													<ScaleIconButton name="teamSpace_listHeaderItemSize" text="大小" font="2" align="right" textcolor="#FF666666"/>
													<Button name="teamSpace_listHeaderItemSizeSortIcon" visible="false" padding="5,5" width="6" height="6"  normalimage="file='..\Image\ic_tab_head_arrowup.png' source='0,0,6,6'" hotimage="file='..\Image\ic_tab_head_arrowup.png' source='0,16,6,22'"/>
												</HorizontalLayout>
											</HorizontalLayout>
										</ListHeaderItem>
										<ListHeaderItem name="teamSpace_listheaderitem_mtime" width="160" minwidth="160" textcolor="#FF666666">
											<HorizontalLayout mousechild="true" padding="15,6,2,0">
												<HorizontalLayout align="right">
													<ScaleIconButton name="teamSpace_listHeaderItemCtime" text="修改日期"  font="2" align="left" textcolor="#FF666666"/>
													<Button name="teamSpace_listHeaderItemCtimeSortIcon" visible="false" padding="5,5" width="6" height="6"  normalimage="file='..\Image\ic_tab_head_arrowup.png' source='0,0,6,6'" hotimage="file='..\Image\ic_tab_head_arrowup.png' source='0,16,6,22'"/>
												</HorizontalLayout>
											</HorizontalLayout>
										</ListHeaderItem>
									</ListHeader>
								</CustomList>
							</VerticalLayout>

							<VerticalLayout>
								<TileLayoutList droptarget="true" bkcolor="#FFFFFFFF" name="teamSpace_tile_FilelistView" menu="true" encloseselect="true" menuxmlfile="teamspaceContextMenu.xml" header="hidden" visible="true" mouse="true" inset="20,8,24,24" childpadding="10" itemsize="122,123" itemshowhtml="true" vscrollbar="true" hscrollbar="false"/>
							</VerticalLayout>

							<VerticalLayout>
								<!--新增搜索-->
								<CustomList name="teamSpace_SearchlistView" menu="true" encloseselect="true" menuxmlfile="teamspaceContextMenu.xml" itemshowhtml="true" vscrollbar="true" hscrollbar="true" itemlinecolor="#FFE5E5E5" itemhotbkcolor="#FFEBEBEB" itemselectedbkcolor="#FFDEF0FF" inset="20,0,20,0">
									<ListHeader height="28" bkimage="file='..\Image\listheaderbk.png' corner='1,1,2,2'">

										<ListHeaderItem name="teamSpace_searchlistheaderitem_name"  width="480" minwidth="480"  sepimage="..\Image\ic_tab_head_devideline.png" sepwidth="1">
											<HorizontalLayout mousechild="false" padding="5,0,2,0">
												<HorizontalLayout width="40">
													<Label name="teamSpace_listHeaderItemName" text="名称" font="2" textcolor="#FF666666" align="center"/>
												</HorizontalLayout>
											</HorizontalLayout>
										</ListHeaderItem>

										<ListHeaderItem width="200" >
											<HorizontalLayout mousechild="false" padding="20,0,2,0">
												<HorizontalLayout>
													<Label name="teamSpace_listHeaderItemSize" text="路径" font="2" textcolor="#FF666666" align="left"/>
												</HorizontalLayout>
												<Control />
											</HorizontalLayout>
										</ListHeaderItem>

									</ListHeader>
								</CustomList>
							</VerticalLayout>
							<!--end 新增搜索-->
						</TabLayout>
					</HorizontalLayout>
				</VerticalLayout>
			</TabLayout>
		</HorizontalLayout>

		<CustomList name="teamSpace_noFiles" visible="false"  droptarget="true" padding="0,10">
			<HorizontalLayout height="200">
				<Control />
				<Label bkimage="..\image\ic_null_files.png" width="180" height="180" padding="5,5,5,5"/>
				<Control />
			</HorizontalLayout>
			<Label align="center" height="18" text="该文件夹为空" textpadding="10,0" font="17"  textcolor="#FF666666"/>
			<Control height="10"/>
			<Label align="center" height="16" text="您可以通过“上传”按钮进行文件上传或直接拖拽上传。" font="12" textcolor="#FF666666"/>
			<Control height="10"/>
			<Control />
		</CustomList>

		<VerticalLayout name="teamSpace_noTeamSpace" visible="false"  droptarget="true" padding="0,0,0,330">
			<HorizontalLayout height="200">
				<Control />
				<Label bkimage="..\image\ic_null_team_space.png" width="180" height="180" padding="5,5,5,5"/>
				<Control />
			</HorizontalLayout>
			<Label align="center" height="18" text="没有团队空间" textpadding="10,0" font="17"  textcolor="#FF666666"/>
			<Control height="10"/>
			<Label align="center" height="16" text="您可以通过“创建团队空间”按钮来新建团队空间。" font="12" textcolor="#FF666666"/>
			<Control height="10"/>
			<Control />
		</VerticalLayout>
	</VerticalLayout>
</Window>